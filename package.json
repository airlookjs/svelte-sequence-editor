{
	"name": "@airlookjs/svelte-sequence-editor",
	"version": "2.0.2",
	"author": "Johan Bichel Lindegaard <public@johan.cc>",
	"license": "MIT",
	"homepage": "https://github.com/airlookjs/svelte-sequence-editor#readme",
	"bugs": {
		"url": "https://github.com/airlookjs/svelte-sequence-editor/issues"
	},
	"keywords": [
		"svelte",
		"timeline",
		"sequence",
		"broadcast",
		"UI",
		"media",
		"video",
		"audio",
		"editor",
		"form"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/airlookjs/svelte-sequence-editor.git"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"build-site": "vite build",
		"preview": "vite build && vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"semantic-release": "semantic-release"
	},
	"devDependencies": {
		"@playwright/test": "^1.40.0",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^11.1.0",
		"@semantic-release/git": "^10.0.1",
		"@sveltejs/adapter-static": "^2.0.3",
		"@sveltejs/kit": "^1.27.6",
		"@sveltejs/package": "^2.2.3",
		"@typescript-eslint/eslint-plugin": "^6.12.0",
		"@typescript-eslint/parser": "^6.12.0",
		"autoprefixer": "^10.4.16",
		"conventional-changelog-eslint": "^5.0.0",
		"dequal": "^2.0.3",
		"eslint": "^8.54.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.35.1",
		"lodash": "^4.17.21",
		"postcss": "^8.4.31",
		"prettier": "^3.1.0",
		"prettier-plugin-svelte": "^3.1.2",
		"semantic-release": "^22.0.8",
		"svelte": "^4.2.8",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^5.1.1",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.6.2",
		"typescript": "^5.3.2",
		"vite": "^4.5.0"
	},
	"peerDependencies": {
		"svelte": "^4.1.2"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js",
	"typesVersions": {
		">4.0": {
			"index": [
				"./dist/index.d.ts"
			]
		}
	},
	"packageManager": "pnpm@8.10.2"
}
