{
	"name": "@airlook/svelte-sequence-editor",
	"version": "0.0.1",
	"author": "Johan Bichel Lindegaard <public@johan.cc>",
	"license": "MIT",
	"homepage": "https://github.com/airlookjs/svelte-sequence-editor#readme",
	"bugs": {
		"url": "https://github.com/airlookjs/svelte-sequence-editor/issues"
	},
	"keywords": [
		"svelte",
		"timeline",
		"editor",
		"form"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/airlookjs/svelte-sequence-editor.git"
	},
	"scripts": {
		"dev": "vite dev",
		"build": "svelte-kit sync && svelte-package",
		"preview": "vite build && vite preview",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@playwright/test": "^1.36.2",
		"@sveltejs/adapter-auto": "2.1.0",
		"@sveltejs/adapter-node": "^1.3.1",
		"@sveltejs/kit": "^1.22.4",
		"@sveltejs/package": "^2.2.0",
		"@typescript-eslint/eslint-plugin": "^6.2.1",
		"@typescript-eslint/parser": "^6.2.1",
		"autoprefixer": "^10.4.14",
		"dequal": "^2.0.3",
		"eslint": "^8.46.0",
		"eslint-config-prettier": "^8.10.0",
		"eslint-plugin-svelte": "^2.32.4",
		"lodash": "^4.17.21",
		"postcss": "^8.4.27",
		"prettier": "^3.0.1",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.1.2",
		"svelte-check": "^3.4.6",
		"svelte-preprocess": "^5.0.4",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.6.1",
		"typescript": "^5.1.6",
		"vite": "^4.4.8"
	},
	"peerDependencies": {
		"svelte": "^4.1.2"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist"
	],
	"svelte": "./dist/index.js",
	"typesVersions": {
		">4.0": {
			"index": [
				"./dist/index.d.ts"
			]
		}
	},
	"dependencies": {}
}
